{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)c1e94c72-c68c-47f5-b1d0-ac8d3462e02d","ParentGuid":"(Guid)00000000-0000-0000-0000-000000000000","Name":"(string)NetVarRx_BAM","Properties":{"@Type":"2c41fa04:IDictionary","@Value":[{"@Key":"(Guid)829a18f2-c514-4f6e-9634-1df173429203","@Value":{"@Type":"829a18f2","@Value":{"ParentObjects":{"@Type":"fa2ee218:IDictionary","@Value":[{"@Key":"(Guid)21af5390-2942-461a-bf89-951aaf6999f1","@Value":"(Guid)fbaa1d9a-65b0-4308-a0ef-4908dddace6b"}]}}}}]},"TypeGuid":"(Guid)6f9dac99-8de1-4efc-8465-68ac443b7d08","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)a9ed5b7e-75c5-4651-af16-d2c27e98cb94","(Guid)3b83b776-fb25-43b8-99f2-3c507c9143fc"]}}},"TypeInfos":{"2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","829a18f2":"{829a18f2-c514-4f6e-9634-1df173429203}","fa2ee218":"{fa2ee218-a39b-4b6d-b249-49dbddbd168a}","Guid":"System.Guid","string":"System.String"}},"object":{"Graph":{"@Type":"6f9dac99","@Value":{"SpecialFunc":{"@Type":"0db3d7bb:Enum","@Value":"None"},"Implementation":{"@Type":"3b83b776","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)aPGN[2]:= DWORD_TO_BYTE(SHR(dwPGN AND 16#00070000,16));\naPGN[1]:= DWORD_TO_BYTE(SHR(dwPGN AND 16#0000FF00,8));\naPGN[0]:= DWORD_TO_BYTE(SHR(dwPGN AND 16#000000FF,0));\n\nIF (dwRxSize <= VAR_CONSTANT.J1939_BAM_LENGTH) THEN \n\tREPEAT\n\t\tTPCM(xEnable:= xEnable, eChannel:= eChannel, xExtended:= TRUE, udiID:= 16#00ECFF00 + usiSA + SHL(BYTE_TO_DWORD(usiPrio), 24));\n\t\tIF TPCM.uiAvailable > 0 THEN\n\t\t\taDataTPCM := TPCM.aData;\n\t\t\tdwRxCounter:= dwRxCounter + 1;\n\t\t\tIF (aDataTPCM[5] = aPGN[0]) AND (aDataTPCM[6] = aPGN[1]) AND (aDataTPCM[7] = aPGN[2]) THEN\n\t\t\t\tTPCM_BAM_MSG_COUNT:= aDataTPCM[3];\n\t\t\t\tTPDT_Counter:= 0;\n\t\t\tEND_IF\n\t\tEND_IF\n\tUNTIL NOT TPCM.uiAvailable > 0 \n\tEND_REPEAT\n\t\n\tREPEAT\t\n\t\tTPDT(xEnable:= xEnable, eChannel:= eChannel, xExtended:= TRUE, udiID:= 16#00EBFF00 + usiSA + SHL(BYTE_TO_DWORD(usiPrio), 24));\n\t\tIF TPDT.uiAvailable > 0 THEN\n\t\t\taDataTPDT := TPDT.aData;\n\t\t\tIF (aDataTPCM[5] = aPGN[0]) AND (aDataTPCM[6] = aPGN[1]) AND (aDataTPCM[7] = aPGN[2]) AND (aDataTPDT[0] = TPDT_Counter+1) THEN\n\t\t\t\tFOR i := 0 TO 6 DO\n\t\t\t\t\t aDataDM1BAM[i+(aDataTPDT[0]-1)*7] := aDataTPDT[i+1];\n\t\t\t\tEND_FOR\n\t\t\t\t\n\t\t\t\tIF TPDT_Counter = TPCM_BAM_MSG_COUNT - 2 THEN\n\t\t\t\t\tprivate_iomgr_memcpy(\t\tpSrc:= ADR(aDataDM1BAM),\n\t\t\t\t\t\t\t\t\t\t\t\tpDst:= pRxAdr, \n\t\t\t\t\t\t\t\t\t\t\t\tiSize:= DWORD_TO_INT(dwRxSize));\n\t\t\t\t\tdwReceivedMsgCounter:= dwReceivedMsgCounter + 1;\n\t\t\t\t\txError:= FALSE;\n\t\t\t\tEND_IF\n\t\t\t\t\n\t\t\t\tTPDT_Counter:= TPDT_Counter + 1;\n\t\t\tEND_IF\t\n\t\tEND_IF\n\tUNTIL NOT TPDT.uiAvailable > 0 \n\tEND_REPEAT\nEND_IF\n\ntimeout_TPCM(IN:= dwRxCounterOLD= dwRxCounter, PT:= tTimeout);\nIF timeout_TPCM.Q THEN\n\txError:= TRUE;\nEND_IF\ndwRxCounterOLD:= dwRxCounter;","LineInfoPersistence":"(string)c1e94c72-c68c-47f5-b1d0-ac8d3462e02d_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)//ifm ecomatic \n//Patrick Paas\n//V1.0.0.5 - 18.Februar 2020\n{attribute 'hide_all_locals'}\n//{attribute 'hide'}\nFUNCTION_BLOCK NetVarRx_BAM\nVAR_INPUT\n\txEnable: BOOL := TRUE;\n\teChannel: ifmDevice.CAN_CHANNEL; \n\tusiPrio: BYTE:= 16#18;\n\tusiSA: BYTE;\t\t\t\t\t\t//SourceAddress of the ECU to listen to\n\tdwPGN:DWORD;\n\tpRxAdr : POINTER TO USINT;;\n\tdwRxSize : DWORD;\n\ttTimeout : TIME := T#1500MS;\nEND_VAR\n\nVAR_OUTPUT\n\txError: BOOL:= TRUE;\n\tdwReceivedMsgCounter: DWORD;\nEND_VAR\n\nVAR\n\ti: UINT;\n \n\ttimeout_TPCM: TON;\n\t \n\tTPCM : ifmRCAN.CAN_Rx;\n\tTPDT: ifmRCAN.CAN_RX;\n\tDM1: ifmRCAN.CAN_RX;\n \n\taDataTPCM: ARRAY [0..7] OF USINT;\n\taDataTPDT: ARRAY [0..7] OF USINT;\n\taDataDM1BAM : ARRAY [0..VAR_CONSTANT.J1939_BAM_LENGTH-1] OF USINT;\n\t\n\tstate: BYTE;\n\tTPDT_Counter: UINT;\n\tTPCM_BAM_MSG_COUNT: UINT;\n\t\n\taPGN: ARRAY [0..2] OF USINT;\n\taData: ARRAY [0..7] OF USINT;\n\tdwRxCounterOLD: DWORD;\n\tdwRxCounter: DWORD;\nEND_VAR\n","LineInfoPersistence":"(string)c1e94c72-c68c-47f5-b1d0-ac8d3462e02d_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)861","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}